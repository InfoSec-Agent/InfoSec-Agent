{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "mocha"
  },
  "devDependencies": {
    "mocha": "*",
    "vite": "^3.0.7"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "chart.js": "^4.4.2",
    "jsdom": "^24.0.0",
    "mocha": "^10.3.0",
    "sqlite3": "^5.1.7",
    "unit.js": "^2.1.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "ignorePatterns": [
      "node_modules/",
      "dist/",
      "wailsjs/"
    ],
    "rules": {
      "linebreak-style":"off",
      "indent": ["error", 2]
    }
  },
  "stylelint": {
    "extends": ["stylelint-config-standard"],
    "ignoreFiles": [
      "node_modules/**",
      "dist/**",
      "wailsjs/**"
    ]
  }
}